[sub_resource type="GDScript" id="GDScript_8vcoy"]
resource_name = "Shop"
script/source = "extends MarginContainer

var shop_item = preload(\"res://assets/shop_item.tscn\")
var shop_list: Array

func set_item(val):
	shop_list = val
	$P/VB/Button.pressed.connect(func(): 
		$P/VB/VB.get_children().map(func(item): 
			item.queue_free()
			self.hide()))
	
	for list in shop_list:
		var item = shop_item.instantiate()
		item.item = list
		$P/VB/VB.add_child(item)
	
	self.show()
"

[sub_resource type="GDScript" id="GDScript_tqwb5"]
resource_name = "LevelUp"
script/source = "extends MarginContainer

var level: Dictionary = {
	\"health\": 0,
	\"damage\": 0,
	\"leap\": 0,
	\"sense\": 0
}

var checkpoint

func _ready():
	$P/VB/Button.pressed.connect(func():
		self.hide())
	for lev in level:
		var node = $P/VB/M/VB.get_node(lev.capitalize())
		node.get_node(\"Button\").pressed.connect(func():
			level[lev] += 1
			node.get_node(\"Label\").text = lev + \" - level: \" + str(level[lev])
			match lev:
				\"health\":
					Player.health_base = 100 + (level[lev] * 10)
				\"damage\":
					Player.damage = 1 + level[lev]
				\"leap\":
					pass
				\"sense\":
					pass
			)
		node.get_node(\"Label\").text = lev + \" - level: \" + str(level[lev])
	
	self.hidden.connect(func():
		checkpoint.get_node(\"Sprite2D\").frame = 0
		Engine.time_scale = 1.0)

func activate(body):
	checkpoint = body
"

