resource_name = "Equipment"
script/source = "extends HBoxContainer

@onready var equipment_item = preload(\"res://assets/equipment_item.tscn\")

func init(type: String):
	self.show()
	Player.equipment[type].map(func(eq):
		var item = equipment_item.instantiate()
		if type != \"consumable\":
			item.init(type, eq)
			item.get_node(\"ItemButton\").pressed.connect(func():
				var data = JSON.parse_string(FileAccess.open(\"res://assets/modules/\"+type+\".json\", FileAccess.READ).get_as_text())
				$Label.text = data[str(eq)][\"desc\"])
		else: 
			item.init(type, eq[0])
			item.get_node(\"ItemButton\").pressed.connect(func():
				var data = JSON.parse_string(FileAccess.open(\"res://assets/modules/\"+type+\".json\", FileAccess.READ).get_as_text())
				$Label.text = data[str(eq[0])][\"desc\"] + \"\\nvalue: \" + str(eq[1]))
			
		$EquipmentList.add_child(item))

func _ready():
	self.hidden.connect(func():
		$Label.text = \"\"
		$EquipmentList.get_children().map(func(child): child.queue_free()))
"

