resource_name = "HUD"
script/source = "extends MarginContainer

var ability_timer: Timer = null

func _ready():
	Player.consumable_value_changed.connect(func(value):
		$Desktop/HB/Consumable/CooldownLabel.text = str(value))
	Player.health_base_changed.connect(func():
		$TopNav/ProgressBar.max_value = Player.health_base)
	Player.health_changed.connect(func():
		$TopNav/ProgressBar/Label.text = str(Player.health)
		$TopNav/ProgressBar.value = Player.health)
	
	$TopNav/MenuButton.pressed.connect(func(): pass)
	$TopNav/BagButton.pressed.connect(func(): Player.get_panel(\"Bag\").show())
	
	Player.leap_charge_changed.connect(func():
		$Desktop/HB/Leap/ValueLabel.text = str(Player.leap_charge))

func _physics_process(_delta):
	if ability_timer: $Desktop/HB/Ability/CooldownLabel.text = str(snapped(ability_timer.time_left, 0.1))
	if Player.leap_charge_base:
		$Desktop/HB/Leap/CooldownLabel.text = str(snapped(Player.leap_cooldown.time_left, 0.1))

func set_ability_timer(timer): ability_timer = timer
"

[sub_resource type="GDScript" id="GDScript_wswhb"]
resource_name = "EquipmentButton"
script/source = "extends Control

var selected: String = \"\"

func _ready():
	$HBoxContainer.get_children().map(func(child):
		child.mouse_entered.connect(func(): selected = child.name.to_lower())
		child.mouse_exited.connect(func(): selected = \"\")
		child.hide())

	$Center.button_down.connect(func():
		$HBoxContainer.get_children().map(func(child):
			if Player.equipped[child.name.to_lower()]:
				child.get_node(\"Label\").text = str(Player.equipped[child.name.to_lower()])
			child.show()))

	$Center.button_up.connect(func():
		if selected:
			Player.get_panel(\"Equipment\").init(selected)
			selected = \"\"
		$HBoxContainer.get_children().map(func(child):
			child.hide()))
"

[sub_resource type="GDScript" id="GDScript_eqt7l"]
resource_name = "Mobile"
script/source = "extends MarginContainer

var velocity: Vector2

func _ready():
	self.visible = Utils.android_mode
	$Control.get_children().map(func(child): child.get_child(0).pressed.connect(func(): pass))

func _input(event):
	if $Analog.button_pressed:
		if event is InputEventScreenDrag:
			velocity = ((event.position - $Analog.global_position) - ($Analog.custom_minimum_size / 2)).normalized()
	else: velocity = Vector2()
"

