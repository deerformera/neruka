[sub_resource type="GDScript" id="GDScript_8vcoy"]
resource_name = "Shop"
script/source = "extends MarginContainer

var shop_item = preload(\"res://assets/shop_item.tscn\")
var shop_list: Array

func set_item(val):
	shop_list = val
	$P/VB/Button.pressed.connect(func(): 
		$P/VB/VB.get_children().map(func(item): 
			item.queue_free()
			self.hide()))
	
	for list in shop_list:
		var item = shop_item.instantiate()
		item.item = list
		$P/VB/VB.add_child(item)
	
	self.show()
"

