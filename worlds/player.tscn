[sub_resource type="GDScript" id="GDScript_7yfqb"]
resource_name = "EquipmentButton"
script/source = "extends MarginContainer

var selected: String = \"\"

func _ready():
	$G.get_children().map(func(child):
		if child.name != \"Center\":
			child.mouse_entered.connect(func(): selected = child.name.to_lower())
			child.mouse_exited.connect(func(): selected = \"\")
			child.hide())
	
	$G/Center.button_down.connect(func():
		$G.get_children().map(func(child):
			if child.name != \"Center\":
				if Player.equipped[child.name.to_lower()]:
					child.get_node(\"Label\").text = str(Player.equipped[child.name.to_lower()])
				child.show()))
	
	$G/Center.button_up.connect(func():
		if selected:
			$\"../Equipment\".activate(selected)
			selected = \"\"
		$G.get_children().map(func(child):
			if child.name != \"Center\":
				child.hide()))
"

