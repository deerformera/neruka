[sub_resource type="GDScript" id="GDScript_ntcqa"]
resource_name = "LevelUp"
script/source = "extends TabContainer

@onready var orb_data = JSON.parse_string(FileAccess.open(\"res://assets/orb.json\", FileAccess.READ).get_as_text())
var orb_item: PackedScene = preload(\"res://assets/orb_use_item.tscn\")
var orbs_used: Array
var selected: String

func _ready():
	$StatsList.get_children().map(func(child):
		child.get_node(\"Label\").text = child.name + \" - Level: \" + str(Player.level[child.name.to_lower()])
		child.get_node(\"Button\").pressed.connect(func(): 
			$OrbsList/LevelLabel.text = child.name + \" - Level: \" + str(Player.level[child.name.to_lower()])
			selected = child.name
			self.current_tab = 1))
	$OrbsList/Button.pressed.connect(func(): self.current_tab = 0)
	
	$OrbsList/HB/Button.pressed.connect(func():
		orbs_used.map(func(orb):
			Player.orb_del(orb[0], orb[1]))
		Player.up_level(selected.to_lower())
		orbs_used.clear()
		$OrbsList/HB/Button.disabled = true
		self.current_tab = 0)
	
	Player.level_changed.connect(func():
		$StatsList.get_children().map(func(child):
			child.get_node(\"Label\").text = child.name + \" - Level: \" + str(Player.level[child.name.to_lower()])
			))
	
	self.draw.connect(func(): self.current_tab = 0)
	$OrbsList/HB/ProgressBar.value_changed.connect(func(value):
		if value >= $OrbsList/HB/ProgressBar.max_value:
			$OrbsList/HB/Button.disabled = false)
	self.tab_selected.connect(func(tab):
		if tab == 0:
			orbs_used.clear()
			$OrbsList/UsedLabel.text = \"\"
			$OrbsList/HB/ProgressBar.value = 0
			$OrbsList/VB.get_children().map(func(child): child.queue_free())
		
		if tab == 1:
			$OrbsList/HB/ProgressBar.max_value = 5 + (Player.level[selected.to_lower()] * 5)
			$OrbsList/VB.get_children().map(func(child): child.queue_free())
			Player.orbs.map(func(orb):
				var orb_item_ins = orb_item.instantiate()
				orb_item_ins.init(orb)
				orb_item_ins.get_node(\"IncrementButton\").pressed.connect(func():
					if $OrbsList/HB/ProgressBar.value < $OrbsList/HB/ProgressBar.max_value:
						orb_add(orb)
						orb_item_ins.value -= 1
						$OrbsList/UsedLabel.text = str(orbs_used)
						$OrbsList/HB/ProgressBar.value += 1)
				orb_item_ins.get_node(\"DecrementButton\").pressed.connect(func():
					if orb_del(orb[0]):
						orb_item_ins.value += 1
						$OrbsList/UsedLabel.text = str(orbs_used)
						$OrbsList/HB/ProgressBar.value -= 1
				)
				$OrbsList/VB.add_child(orb_item_ins))
		)

func orb_add(orb) -> void:
	for o in orbs_used:
		if o[0] == orb[0]:
			o[1] += 1
			return
	orbs_used.append([orb[0], 1])

func orb_del(id) -> bool:
	for o in orbs_used:
		if o[0] == id:
			if o[1] >= 1:
				o[1] -= 1
				if o[1] == 0: orbs_used.erase(o)
				return true
	return false
"

