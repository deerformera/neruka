[sub_resource type="GDScript" id="GDScript_c2w07"]
resource_name = "Equipment"
script/source = "extends MarginContainer

func _ready():
	$P/VB/HB/Button.pressed.connect(func(): self.hide())

func activate(type: String):
	$P/VB/M/HB/EquipmentList.get_children().map(func(node): node.queue_free())
	$P/VB/HB/Label.text = type
	Player.equipment[type].map(func(id: int):
		var item = load(\"res://assets/equipment_item.tscn\")
		item = item.instantiate()
		item.set_item(type, id)
		item.get_node(\"ItemButton\").pressed.connect(func():
			var data = JSON.parse_string(FileAccess.open(\"res://assets/modules/\"+type+\".json\", FileAccess.READ).get_as_text())
			$P/VB/M/HB/Label.text = data[str(id)][\"desc\"])
		$P/VB/M/HB/EquipmentList.add_child(item))
	self.show()
"

